{
  "manifest_version": 3,
  "name": "IP Opener",
  "version": "1.1",
  "description": "Enter a sitename and open it in a new tab, with editable mappings.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["ip-mapping.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
